                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT ac.actor_key, ac.rental_key, ac.inventory_key, ac.rental_date_key, ac.RentalFilm, af.ActorFamous, ac.ActorCategory
FROM     (SELECT a.actor_key, a.actor_id, a.first_name AS actor_first_name, a.last_name AS actor_last_name, a.film_id, COUNT(DISTINCT r.rental_id) * r.amount AS RentalFilm, category.ActorCategory, r.rental_key, d.inventory_key, 
                                    dd.date_key AS rental_date_key
                  FROM      DimActor AS a INNER JOIN
                                        (SELECT DISTINCT a1.actor_id, COUNT(DISTINCT d1.category_name) AS ActorCategory
                                         FROM      DimActor AS a1 INNER JOIN
                                                           DimInventory AS d1 ON a1.film_id = d1.film_id
                                         GROUP BY a1.actor_id) AS category ON a.actor_id = category.actor_id INNER JOIN
                                    DimInventory AS d ON a.film_id = d.film_id INNER JOIN
                                    DimRental AS r ON d.Inventory_id = r.inventory_id INNER JOIN
                                    DimDate AS dd ON CONVERT(date, r.rental_date) = CONVERT(date, dd.[Date])
                  GROUP BY a.actor_key, a.actor_id, a.first_name, a.last_name, a.film_id, r.amount, category.ActorCategory, r.rental_key, d.inventory_key, dd.date_key) AS ac INNER JOIN
