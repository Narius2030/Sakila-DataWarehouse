                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT s.staff_key, r.rental_key, s.store_id, s.address_store_id, a.city, a.district, a.country, s.full_name, r.amount,
                      (SELECT COUNT(rental_id) AS Expr1
                       FROM      DimRental
                       WHERE   (staff_id = s.staff_id)) AS quantity,
                      (SELECT SUM(amount) AS Expr1
                       FROM      DimRental AS DimRental_1
                       WHERE   (staff_id = s.staff_id)) AS revenue, (DAY(r.rental_date) + MONTH(r.rental_date) * 100) + YEAR(r.rental_date) * 10000 AS rental_date_key, (DAY(r.payment_date) + MONTH(r.payment_date) * 100) + YEAR(r.payment_date) 
                  * 10000 AS payment_date_key
FROM     DimRental AS r INNER JOIN
                  DimStaff AS s ON s.staff_id = r.staff_id INNER JOIN
                  DimAddress AS a ON a.address_id = s.address_store_id</property>
